    "@nestjs/testing": "^9.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.4.1",
    "@types/passport-jwt": "^3.0.8",
