  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:ci": "yarn build && cp package.json yarn.lock nginx.conf.template dist && tar -czf dist.tar.gz -C dist .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:worker": "MMM_WORKER_MODE=true yarn start:dev",
    "deploy": "./scripts/deploy/main.sh",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register -r dotenv-expand/config ./node_modules/typeorm/cli -f src/typeorm-cli.config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^10.0.1",
