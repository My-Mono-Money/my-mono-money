{
  "name": "frontend",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.2.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@mui/x-data-grid": "^6.9.1",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dates": "^21.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-loader-spinner": "^5.3.4",
    "react-router-dom": "6.14.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "typescript": "^5.1.3",
    "use-debounce": "^9.0.4",
    "web-vitals": "^3.3.2",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "deploy:development": "firebase deploy --only hosting:development",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "eject": "react-scripts eject",
    "ts-coverage": "typescript-coverage-report"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080/v1",
  "devDependencies": {
    "@types/react-dates": "^21.8.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@types/css-mediaquery": "^0.1.1",
    "css-mediaquery": "^0.1.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "2.8.8"
  }
}
